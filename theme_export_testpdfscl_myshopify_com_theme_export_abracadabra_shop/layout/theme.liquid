      }

      @media (max-width: 767px) {
        .header-grid:has(.header-right.search-open) {
          grid-template-columns: 1fr;
        }

        .header-grid:has(.header-right.search-open) .header-logo-cell {
          display: none !important;
        }

        .header-right.search-open {
          justify-self: stretch;
        }

        .header-right.search-open .search-wrapper {
          display: block;
          width: 100%;
        }

        .header-right.search-open .header-search,
        .header-right.search-open .header-search input {
          width: 100%;
        }
      }
    </style>

    <script>
      tailwind.config = {
        theme: {
          container: {
            center: true,
            padding: '1rem',
          },
          extend: {
            colors: {
              brand: '#111827',
              primary: '#647167',
              surface: '#D5CFC4',
              border: '#E5E7EB',
              customGray: '#2E2E2D',
            },
            borderRadius: { xl: '12px', '2xl': '16px' },
            boxShadow: { soft: '0 10px 30px rgba(0,0,0,.08)' },

            fontFamily: {
              sans: ['"Source Sans 3"'],
            },
          },
        },
      };
    </script>

    <script src="{{ 'common.js' | asset_url }}" defer></script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@splidejs/splide@4.1.4/dist/css/splide.min.css">
    <script defer src="https://cdn.jsdelivr.net/npm/@splidejs/splide@4.1.4/dist/js/splide.min.js"></script>
    <script defer src="https://www.youtube.com/iframe_api"></script>
    <!-- DIESE ZEILE HINZUFÃœGEN -->

    <script src="{{ 'wishlist.js' | asset_url }}" defer></script>
    <script src="{{ 'cart.js' | asset_url }}" defer></script>
    <script src="{{ 'mega-menu.js' | asset_url }}" defer></script>
    <script src="{{ 'product-carousel.js' | asset_url }}" defer></script>
    <script src="{{ 'header.js' | asset_url }}" defer></script>

    <link rel="stylesheet" href="https://e-catalog-frontend.app615423e.interbranche.com/assets/index.css?v=2">
    <script
      defer
      type="module"
      src="https://e-catalog-frontend.app615423e.interbranche.com/assets/index.js?v=2"
    ></script>
    <script
      defer
      type="module"
      src="https://warranty-frontend.app615423e.interbranche.com/assets/index.js?v=1"
    ></script>

    {{ content_for_header }}
  </head>

  <body class="font-sans bg-white min-h-screen flex flex-col">
    <a id="top"></a>

    <header class="sticky top-0 z-50 bg-white/95 backdrop-blur relative">
      {% section 'announcement-bar' %}
      <div class="container max-w-[var(--container)] mx-auto h-[70px] grid grid-cols-[auto,1fr,auto] md:grid-cols-3 items-center header-grid">
        <div class="col-start-1 header-left">
          {% section 'mega-menu' %}
        </div>

        <div class="col-start-2 justify-self-center sm:block lg:!block header-logo-cell">
          {% section 'header-logo' %}
        </div>

        <div class="col-start-3 header-right justify-self-end self-center flex items-center">
          <!-- search: desktop-only, anchored for dropdown -->
          <div class="relative hidden md:block md:mr-6 search-wrapper">
            <div class="header-search">
              {% section 'header-search-container' %}
            </div>
          </div>

          <!-- icons -->
          <div class="header-icons flex md:items-baseline h-[20px] leading-none text-[0] gap-[20px]">
            {% section 'header-search-button' %}
            {% section 'header-account-button' %}
            {% section 'header-wishlist-button' %}
            {% section 'header-cart-button' %}
          </div>
        </div>
      </div>
    </header>

    <main id="MainContent" class="flex-1">
      {{ content_for_layout }}
    </main>

    {% comment %} {% section 'cart-banner' %} {% endcomment %}

    {% section 'footer' %}

    <a id="back-to-top" aria-label="{{ 'general.back-to-top' | t }}" href="#top"></a>

    {% render 'roomio-lightbox' %}
    {% render 'ecatalog-globals' %}

    <script src="{{ 'cart-badge.js' | asset_url }}" defer></script>
    <script src="{{ 'back-to-top.js' | asset_url }}" defer></script>
  </body>
</html>
